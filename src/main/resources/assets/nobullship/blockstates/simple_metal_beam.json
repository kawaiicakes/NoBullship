{
  "multipart": [
    {
      "apply": {
        "model": "nobullship:block/simple_metal_beam"
      },
      "when": {
        "north": "true|false"
      }
    },
    {
      "apply": {
        "model": "nobullship:block/simple_metal_beam_side"
      },
      "when": {
        "north": "true"
      }
    },
    {
      "apply": {
        "model": "nobullship:block/simple_metal_beam_side",
        "y": 90
      },
      "when": {
        "east": "true"
      }
    },
    {
      "apply": {
        "model": "nobullship:block/simple_metal_beam_side",
        "y": 180
      },
      "when": {
        "south": "true"
      }
    },
    {
      "apply": {
        "model": "nobullship:block/simple_metal_beam_side",
        "y": 270
      },
      "when": {
        "west": "true"
      }
    },
    {
      "apply": {
        "model": "nobullship:block/simple_metal_beam_side",
        "x": 270
      },
      "when": {
        "up": "true"
      }
    },
    {
      "apply": {
        "model": "nobullship:block/simple_metal_beam_side",
        "x": 90
      },
      "when": {
        "down": "true"
      }
    }
  ]
}